generator client {
  provider   = "prisma-client-js"
  output     = "../.prisma/client"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model node {
  nodeID    String @id
  xcoord    Int
  ycoord    Int
  floor     String
  building  String
  nodeType  String
  longName  String
  shortName String
  edges     String
}

model edge {
  edgeID      String @id
  startNodeID String
  endNodeID   String
}

model path {
  uniqueID    Int    @id @default(autoincrement())
  startNodeID String
  endNodeID   String
  nodes       String
}
