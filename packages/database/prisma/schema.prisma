
generator client {
  provider   = "prisma-client-js"
  output     = "../.prisma/client"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model node {
  nodeID    String  @id
  xcoord    Int
  ycoord    Int
  floor     String
  building  String
  nodeType  String
  longName  String
  shortName String
  edges     String
  blocked   Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime?
}

model edge {
  edgeID      String @id
  startNodeID String
  endNodeID   String
  createdAt DateTime @default(now())
  updatedAt DateTime?
}

model path {
  uniqueID    Int    @id @default(autoincrement())
  startNodeID String
  endNodeID   String
  createdAt DateTime @default(now())
  //empId Int @default(1)
  //employee employee @relation(fields: [empId], references: [ID])
}
model service {
  id Int @id
  location String
  status String
  Priority String
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee employee @relation(fields: [empId], references: [ID])
}


model flower {
  id          Int      @id @default(autoincrement())
  patientName String
  location    String
  senderName  String
  cardMessage String
  flowerType  String
  status      String
  priority    String
  date        DateTime @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model gift {
  id            Int    @id @default(autoincrement())
  recipientName String
  location      String
  message       String
  giftSize      String
  status        String
  priority      String
  wrapping      String
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model interpreter {
  id             Int    @id @default(autoincrement())
  clientName     String
  language       String
  duration       String
  location       String
  status         String
  priority       String
  additionalInfo String
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])

}

model security {
  id               Int     @id @default(autoincrement())
  employeeName     String
  employeeID       Int
  reqPriority      String
  location         String
  requestType      String
  reqStatus        String
  alertAuthorities Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model medicine {
  id               Int      @id @default(autoincrement())
  patientName      String
  location         String
  patientCondition String
  drugName         String
  drugQuantity     String
  status           String
  priority         String
  date             DateTime @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model sanitation {
  id             Int    @id @default(autoincrement())
  patientName    String
  location       String
  serviceType    String
  status         String
  priority       String
  additionalInfo String
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model roomschedule {
  id           Int    @id @default(autoincrement())
  employeeName String
  priority     String
  location     String
  serviceType  String
  status       String
  startTime    String
  endTime      String
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model medicaldevice {
  id             Int     @id @default(autoincrement())
  employeeName   String
  priority       String
  location       String
  selectedDevice String
  status         String
  withBalloons   Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime?
  //empId Int @default(1)
  //employee Int @relation(fields: [empId], references: [ID])
}

model employee {
  ID     Int      @id @default(autoincrement())
  username  String
  password  String
  fName     String
  lName     String
  position  String
  admin Boolean @default(false)
  createdAt DateTime @default(now())
}
